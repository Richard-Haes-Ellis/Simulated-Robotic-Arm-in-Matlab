

figure;


splt = subplot(3,3,1);
plt = plot(t,q(:,1),t,q(:,2),t,q(:,3));
set(plt(1),'DisplayName','Articulacion q_1');
set(plt(2),'DisplayName','Articulacion q_2');
set(plt(3),'DisplayName','Articulacion q_3');
legend(splt,'show');
grid;
xlabel('t');
ylabel('rad');

splt = subplot(3,3,2);
plt = plot(t,qd(:,1),t,qd(:,2),t,qd(:,3));
set(plt(1),'DisplayName','Articulacion q_1');
set(plt(2),'DisplayName','Articulacion 2');
set(plt(3),'DisplayName','Articulacion 3');
legend(splt,'show');
grid;
xlabel('t');
ylabel('rad/s');

splt = subplot(3,3,3);
plt = plot(t,qdd(:,1),t,qdd(:,2),t,qdd(:,3));
set(plt(1),'DisplayName','Articulacion 1');
set(plt(2),'DisplayName','Articulacion 2');
set(plt(3),'DisplayName','Articulacion 3');
legend(splt,'show');
grid;
xlabel('t');
ylabel('rad/sÂ²');


